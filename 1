#include <stdio.h>
#include <math.h>
int main()
{
    double x, t, result;
    printf  ("\nВведите число 1 "); 
    scanf  ("%lf",&x);
    printf  ("\nВведите число 2 ");
    scanf  ("%lf",&t);
    printf  ("\nVashe srednee ariphmeticheskoe: ");
    result=round((x + t) / 2;
    printf  ("%.3lf",result);
    result=round(sqrt(x*t);
    printf  ("\nVashe srednee geometricheskoe: ");
    printf ("%.3lf",result);
}
